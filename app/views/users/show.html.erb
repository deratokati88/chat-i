<h1><%= @user.name %>さんの投稿一覧</h1>
<div class="contents_row">
  <% @charas.each do |chara| %>
    <div class="content_post" >
      <div>
     <%= link_to image_tag(chara.image.url, class: "chara_image"), chara_path(chara.id) %>
      </div>
      <span class="name"><%= chara.name %></span>
    </div>
  <% end %>
</div>